{
  "name": "saeven/zf2-circlical-recaptcha",
  "description": "ZF2 module that lets you easily incorporate Google's new, simpler recaptcha (I am not a robot)",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/Saeven/zf2-circlical-recaptcha",
  "keywords": [
    "Laminas",
    "google",
    "recaptcha",
    "form",
    "captcha"
  ],
  "authors": [
    {
      "name": "Alexandre Lemaire",
      "email": "alemaire@circlical.com",
      "homepage": "http://circlical.com/",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.2",
    "laminas/laminas-servicemanager": "^3.4",
    "laminas/laminas-inputfilter": "^2.10",
    "laminas/laminas-form": "^2.14",
    "laminas/laminas-view": "^2.11",
    "laminas/laminas-mvc": "^3.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpspec/phpspec": "^6.1",
    "phpunit/php-file-iterator": "^3.0",
    "codacy/coverage": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "CirclicalRecaptcha\\": "src/"
    }
  }
}